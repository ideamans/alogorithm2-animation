{
  "name": "alogorithm2",
  "description": "ideaman's Inc logo mark generator v2.",
  "version": "2.1.0",
  "type": "module",
  "typings": "build/index.d.ts",
  "module": "build/index.js",
  "license": "MIT",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "tsc",
    "fix": "run-s fix:*",
    "fix:lint": "eslint --fix",
    "fix:prettier": "prettier --write src",
    "test": "run-s build test:*",
    "test:lint": "eslint src",
    "test:prettier": "prettier src --list-different",
    "test:cspell": "cspell \"{README.md,.github/*.md,src/**/*.ts}\"",
    "test:unit": "nyc --silent ava",
    "watch": "nyc --silent ava --watch",
    "start": "run-s build && node build/start.js",
    "dev": "vite --config vite.config.ts dev",
    "dev:react": "vite --config vite.react.config.ts",
    "dev:vue": "vite --config vite.vue.config.ts",
    "test:react": "vitest run src/react/__tests__",
    "test:react:watch": "vitest watch src/react/__tests__",
    "test:vue": "vitest run --config vitest.vue.config.ts src/vue/__tests__",
    "test:vue:watch": "vitest watch --config vitest.vue.config.ts src/vue/__tests__"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.7.5",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/sort-json": "^2.0.3",
    "@types/text-to-svg": "^3.1.4",
    "@types/trianglify": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "@vitejs/plugin-react": "^4.5.2",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/ui": "^3.2.3",
    "@vue/test-utils": "^2.4.6",
    "ava": "^6.1.3",
    "cspell": "^8.14.4",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jsdom": "^26.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "standard-version": "^9.5.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "vite": "^6.3.5",
    "vitest": "^3.2.3",
    "vue": "^3.5.16"
  },
  "dependencies": {
    "blobs": "^2.3.0-beta.2",
    "fastify": "^5.0.0",
    "pino": "^9.4.0",
    "pino-pretty": "^11.2.2",
    "sharp": "^0.33.5",
    "sort-json": "^2.0.1",
    "svg-path-bbox": "^2.1.0",
    "svg-pathdata": "^7.1.0",
    "svgson": "^5.3.1",
    "text-to-svg": "^3.1.5",
    "trianglify": "^4.1.1"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "printWidth": 120
  }
}
